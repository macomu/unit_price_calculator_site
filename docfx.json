{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "docs/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "docs",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "UnitPriceCalculatorDocs",
      "_appTitle": "UnitPriceCalculatorDocs",
      "_enableSearch": false,
      "pdf": false
    }
  }
}